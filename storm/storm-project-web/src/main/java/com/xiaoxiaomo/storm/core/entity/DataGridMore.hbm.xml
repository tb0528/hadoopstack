<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
     <class name="com.xiaoxiaomo.storm.core.entity.DataGridMore" table="data_grid_more">
        <id name="id" type="java.lang.Integer"> 
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="sqlstr" type="java.lang.String">
            <column name="sqlstr" />
        </property>     
         <property name="name" type="java.lang.String">
            <column name="name" />
        </property>   
         <property name="fieldname" type="java.lang.String">
            <column name="fieldname" />
        </property>  
        <property name="fieldsql" type="java.lang.String">
            <column name="fieldsql" />
        </property>      
    </class>
</hibernate-mapping>
